local Button = Tab:CreateButton({
   Name = "Aim Assist",
   Callback = function()
   local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
 
local player = Players.LocalPlayer
local mouse = player:GetMouse()
local camera = workspace.CurrentCamera
 
local function getClosestPlayerToMouse()
    local closestPlayer = nil
    local closestDistance = math.huge
 
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("UpperTorso") then
            local screenPoint = camera:WorldToScreenPoint(otherPlayer.Character.UpperTorso.Position)
            local distance = (Vector2.new(screenPoint.X, screenPoint.Y) - Vector2.new(mouse.X, mouse.Y)).Magnitude
            if distance < closestDistance then
                closestDistance = distance
                closestPlayer = otherPlayer
            end
        end
    end
 
    return closestPlayer
end
 
local function attachPartToPlayer(player)
    local character = player.Character
    if not character or not character:FindFirstChild("UpperTorso") then return end
 
    local part = Instance.new("Part")
    part.Size = Vector3.new(0.1, 0.1, 0.1)
    part.Anchored = false
    part.CanCollide = false
    part.Transparency = 1
    part.Parent = workspace
 
    local weld = Instance.new("WeldConstraint")
    weld.Part0 = character.UpperTorso
    weld.Part1 = part
    weld.Parent = part
 
    part.CFrame = character.UpperTorso.CFrame
 
    return part
end
 
local function lookAtPart(part)
    local offset = Vector3.new(0, 0, 5)
    local targetPosition = part.Position - (part.CFrame.LookVector * offset.Z)
    camera.CameraType = Enum.CameraType.Scriptable
 
    local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
    local targetCFrame = CFrame.new(targetPosition, part.Position)
    local tween = TweenService:Create(camera, tweenInfo, {CFrame = targetCFrame})
    tween:Play()
end
 
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.Q then
        local closestPlayer = getClosestPlayerToMouse()
        if closestPlayer then
            local part = attachPartToPlayer(closestPlayer)
            if part then
                lookAtPart(part)
            end
        end
    end
end)
   end,
})


